{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": "^8.4",
    "ext-ctype": "*",
    "ext-curl": "*",
    "ext-iconv": "*",
    "ext-openssl": "*",
    "ext-zip": "*",
    "doctrine/dbal": "^3.7",
    "doctrine/doctrine-bundle": "*",
    "doctrine/doctrine-migrations-bundle": "^3.3",
    "doctrine/orm": "*",
    "james-heinrich/getid3": "^1.9",
    "nelmio/api-doc-bundle": "^5.8",
    "nelmio/cors-bundle": "^2.2",
    "openpayu/openpayu": "^2.3",
    "predis/predis": "^2.2",
    "psr/log": "^2.0",
    "symfony/asset": "6.4.*",
    "symfony/cache": "6.4.*",
    "symfony/config": "6.4.*",
    "symfony/console": "6.4.*",
    "symfony/dependency-injection": "6.4.*",
    "symfony/dotenv": "6.4.*",
    "symfony/filesystem": "6.4.*",
    "symfony/flex": "^2",
    "symfony/framework-bundle": "6.4.*",
    "symfony/google-mailer": "6.4.*",
    "symfony/mailer": "6.4.*",
    "symfony/maker-bundle": "^1.65",
    "symfony/monolog-bundle": "^3.7",
    "symfony/property-access": "6.4.*",
    "symfony/proxy-manager-bridge": "6.4.*",
    "symfony/runtime": "6.4.*",
    "symfony/security-bundle": "6.4.*",
    "symfony/serializer": "6.4.*",
    "symfony/translation": "6.4.*",
    "symfony/twig-bundle": "6.4.*",
    "symfony/uid": "6.4.*",
    "symfony/validator": "6.4.*",
    "symfony/yaml": "6.4.*",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/twig": "^2.12|^3.0",
    "vonage/client": "^4.0",
    "web3p/ethereum-tx": "dev-master",
    "web3p/web3.php": "dev-master"
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "phpstan/extension-installer": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "rector": "php vendor/bin/rector process --config=./rector.php --dry-run --memory-limit=16G",
    "rector:fix": "php vendor/bin/rector process --config=./rector.php --memory-limit=16G",
    "cs": "php vendor/bin/php-cs-fixer fix --config=./php-cs-fixer.php --dry-run",
    "cs:fix": "php vendor/bin/php-cs-fixer fix --config=./php-cs-fixer.php",
    "phpstan": "php vendor/bin/phpstan analyse -c phpstan.neon.dist --memory-limit=16G"
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "6.4.*"
    }
  },
  "require-dev": {
    "dama/doctrine-test-bundle": "v7.2.*",
    "doctrine/doctrine-fixtures-bundle": "*",
    "fakerphp/faker": "*",
    "friendsofphp/php-cs-fixer": "^3.90",
    "guzzlehttp/psr7": "^2.8",
    "kubawerlos/php-cs-fixer-custom-fixers": "^3.35",
    "phpstan/extension-installer": "^1.4",
    "phpstan/phpstan": "^2.1",
    "phpstan/phpstan-deprecation-rules": "^2.0",
    "phpstan/phpstan-doctrine": "^2.0",
    "phpstan/phpstan-strict-rules": "^2.0",
    "phpstan/phpstan-symfony": "^2.0",
    "phpunit/phpunit": "^10",
    "rector/rector": "^2.2",
    "roave/security-advisories": "dev-master",
    "squizlabs/php_codesniffer": "*",
    "symfony/browser-kit": "6.4.*",
    "symfony/css-selector": "6.4.*",
    "symfony/debug-bundle": "6.4.*",
    "symfony/panther": "*",
    "symfony/phpunit-bridge": "^6.4",
    "symfony/stopwatch": "6.4.*",
    "symfony/web-profiler-bundle": "6.4.*"
  }
}
